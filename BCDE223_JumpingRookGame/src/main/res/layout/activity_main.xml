<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/loc00"
        android:layout_width="83dp"
        android:layout_height="83dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/three" />

    <ImageView
        android:id="@+id/loc01"
        android:layout_width="83dp"
        android:layout_height="83dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toEndOf="@+id/loc00"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/two" />

    <ImageView
        android:id="@+id/loc02"
        android:layout_width="83dp"
        android:layout_height="83dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toEndOf="@+id/loc01"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/three" />

    <ImageView
        android:id="@+id/loc03"
        android:layout_width="83dp"
        android:layout_height="83dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toEndOf="@+id/loc02"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/two" />

    <ImageView
        android:id="@+id/loc10"
        android:layout_width="83dp"
        android:layout_height="83dp"
        android:layout_marginStart="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loc00"
        app:srcCompat="@drawable/four" />

    <ImageView
        android:id="@+id/loc11"
        android:layout_width="83dp"
        android:layout_height="83dp"
        app:layout_constraintStart_toEndOf="@+id/loc10"
        app:layout_constraintTop_toBottomOf="@+id/loc01"
        app:srcCompat="@drawable/one" />

    <ImageView
        android:id="@+id/loc12"
        android:layout_width="83dp"
        android:layout_height="83dp"
        app:layout_constraintStart_toEndOf="@+id/loc11"
        app:layout_constraintTop_toBottomOf="@+id/loc02"
        app:srcCompat="@drawable/one" />

    <ImageView
        android:id="@+id/loc13"
        android:layout_width="83dp"
        android:layout_height="83dp"
