        app:layout_constraintTop_toBottomOf="@+id/loc20"
        app:srcCompat="@drawable/one" />

    <ImageView
        android:id="@+id/loc31"
        android:layout_width="83dp"
        android:layout_height="83dp"
        app:layout_constraintStart_toEndOf="@+id/loc30"
        app:layout_constraintTop_toBottomOf="@+id/loc21"
        app:srcCompat="@drawable/three" />

    <ImageView
        android:id="@+id/loc32"
        android:layout_width="83dp"
        android:layout_height="83dp"
        app:layout_constraintStart_toEndOf="@+id/loc31"
        app:layout_constraintTop_toBottomOf="@+id/loc22"
        app:srcCompat="@drawable/one" />

    <ImageView
        android:id="@+id/loc33"
        android:layout_width="83dp"
        android:layout_height="83dp"
        app:layout_constraintStart_toEndOf="@+id/loc32"
        app:layout_constraintTop_toBottomOf="@+id/loc23"
        app:srcCompat="@drawable/zero" />

    <ImageView
        android:id="@+id/backMove"
        android:layout_width="83dp"
        android:layout_height="83dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="64dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loc30"
        app:srcCompat="@drawable/backmove" />

    <ImageView
        android:id="@+id/resetMap"
        android:layout_width="83dp"
        android:layout_height="83dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/backMove"
        app:layout_constraintTop_toBottomOf="@+id/loc33"
        app:srcCompat="@drawable/restart_level" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="73dp"
        android:layout_height="73dp"
        app:layout_constraintBottom_toBottomOf="@+id/loc00"
        app:layout_constraintEnd_toEndOf="@+id/loc00"
        app:layout_constraintStart_toStartOf="@+id/loc00"
        app:layout_constraintTop_toTopOf="@+id/loc00"
        app:srcCompat="@drawable/berserker_chess_piece_norway" />

</androidx.constraintlayout.widget.ConstraintLayout>