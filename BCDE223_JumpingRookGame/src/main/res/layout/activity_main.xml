    <ImageView
        android:id="@+id/loc22"
        android:layout_width="83dp"
        android:layout_height="83dp"
        app:layout_constraintStart_toEndOf="@+id/loc21"
        app:layout_constraintTop_toBottomOf="@+id/loc12"
        app:srcCompat="@drawable/two" />

    <ImageView
        android:id="@+id/loc23"
        android:layout_width="83dp"
        android:layout_height="83dp"
